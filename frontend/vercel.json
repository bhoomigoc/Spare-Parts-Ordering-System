{
  "builds": [
    {
      "src": "build/static/**",
      "use": "@vercel/static"
    },
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/index.html"
    }
  ],
  "functions": {},
  "regions": ["bom1"]
}